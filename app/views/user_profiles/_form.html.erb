<%= simple_form_for(@user_profile) do |f| %>
  <%= f.error_notification %>
  <div class="form-group">
    <%= f.association :user, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :name, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :website, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :twitter, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :linkedin, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :github, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :startupcommunity, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :google_plus, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :bio, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :title, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :image, input_html: { class: 'form-control'} %>
  </div>

  <%= f.button :submit %>
  
<% end %>
